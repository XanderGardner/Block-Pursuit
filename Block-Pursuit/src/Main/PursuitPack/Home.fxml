<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.PursuitPack.HomeController">
   <children>
      <AnchorPane fx:id="backdrop" prefHeight="620.0" prefWidth="620.0" style="-fx-background-color: black;">
         <children>
            <Label layoutX="75.0" layoutY="80.0" prefHeight="67.0" prefWidth="473.0" text="Block Pursuit" textFill="WHITE">
               <font>
                  <Font name="OCR A Extended" size="60.0" />
               </font>
            </Label>
            <Button layoutX="115.0" layoutY="260.0" mnemonicParsing="false" onAction="#normalClicked" style="-fx-background-color: green;" text="Normal" textFill="WHITE">
               <font>
                  <Font name="OCR A Extended" size="36.0" />
               </font>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Button>
            <Button layoutX="320.0" layoutY="375.0" mnemonicParsing="false" onAction="#insaneClicked" style="-fx-background-color: darkred;" text="Insane" textFill="WHITE">
               <font>
                  <Font name="OCR A Extended" size="36.0" />
               </font>
            </Button>
            <Label layoutX="210.0" layoutY="170.0" text="BY XANDER GARDNER" textFill="WHITE">
               <font>
                  <Font name="OCR A Extended" size="22.0" />
               </font>
            </Label>
            <Button fx:id="showTarButton" alignment="CENTER" layoutX="215.0" layoutY="500.0" mnemonicParsing="false" onAction="#showTarClicked" style="-fx-background-color: white;" text="Show Target: OFF">
               <font>
                  <Font name="OCR A Extended" size="18.0" />
               </font>
            </Button>
         </children>
         <cursor>
            <Cursor fx:constant="CROSSHAIR" />
         </cursor>
      </AnchorPane>
      <AnchorPane fx:id="mapBackdrop" prefHeight="620.0" prefWidth="620.0" style="-fx-background-color: black;" visible="false">
         <children>
            <Label layoutX="117.0" layoutY="70.0" text="Map Select" textFill="WHITE">
               <font>
                  <Font name="OCR A Extended" size="64.0" />
               </font>
            </Label>
            <Button layoutX="165.0" layoutY="190.0" mnemonicParsing="false" onAction="#arenaC" prefWidth="300.0" style="-fx-background-color: lightgreen;" text="Arena">
               <font>
                  <Font name="OCR A Extended" size="36.0" />
               </font>
            </Button>
            <Button layoutX="165.0" layoutY="280.0" mnemonicParsing="false" onAction="#forestC" prefWidth="300.0" style="-fx-background-color: green;" text="Forest" textFill="WHITE">
               <font>
                  <Font name="OCR A Extended" size="36.0" />
               </font>
            </Button>
            <Button layoutX="165.0" layoutY="370.0" mnemonicParsing="false" onAction="#mazeC" prefWidth="300.0" style="-fx-background-color: pink;" text="Maze">
               <font>
                  <Font name="OCR A Extended" size="36.0" />
               </font>
            </Button>
            <Button layoutX="165.0" layoutY="460.0" mnemonicParsing="false" onAction="#deathC" prefWidth="300.0" style="-fx-background-color: red;" text="Death Realm" textFill="WHITE">
               <font>
                  <Font name="OCR A Extended" size="36.0" />
               </font>
            </Button>
            <Button layoutX="243.0" layoutY="547.0" mnemonicParsing="false" onAction="#homeC" style="-fx-background-color: white;" text="Return Home">
               <font>
                  <Font name="OCR A Extended" size="18.0" />
               </font>
            </Button>
         </children>
         <cursor>
            <Cursor fx:constant="CROSSHAIR" />
         </cursor>
      </AnchorPane>
   </children>
</StackPane>
